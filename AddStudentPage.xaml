<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Lab3_json.AddStudentPage"
             Title="AddStudentPage">
  <StackLayout>
        <Entry Placeholder="Id" x:Name="entryId" />
        <Entry Placeholder="Name" x:Name="entryName" />
        <Entry Placeholder="Surname" x:Name="entrySurname" />
        <Entry Placeholder="Faculty" x:Name="entryFaculty" />
        <Entry Placeholder="StudyingYear" x:Name="entryStudyingYear" />

        <Button Text="OK" Clicked="OnOkClicked" />
    </StackLayout>
</ContentPage>
